option, -echo;
call, file = '/home/eirik/CERN/correction_test/correction/model_dir/macros/general.macros.madx';
call, file = '/home/eirik/CERN/correction_test/correction/model_dir/macros/lhc.macros.madx';
title, "Model from Lukas :-)";
call, file = '/home/eirik/CERN/omc3/omc3/model/accelerators/lhc/2018/main.seq';

exec, define_nominal_beams();
call, file = '/home/eirik/CERN/correction_test/correction/model_dir/modifiers.madx';
exec, cycle_sequences();
xing_angles = 0;
if(xing_angles==1){
    exec, set_crossing_scheme_ON();
}else{
    exec, set_default_crossing_scheme();
}
use, sequence = LHCB1;
option, echo;
exec, match_tunes(0.275, 0.295, 1);
exec, coupling_knob(1);
use_acd=1;
use_adt=0;
exec, do_twiss_monitors(LHCB1, '/home/eirik/CERN/correction_test/correction/model_dir/twiss.dat', 0.0);
exec, do_twiss_elements(LHCB1, '/home/eirik/CERN/correction_test/correction/model_dir/twiss_elements.dat', 0.0);
if(use_acd == 1){
exec, twiss_ac_dipole(0.275, 0.295, 0.263, 0.31, 1, '/home/eirik/CERN/correction_test/correction/model_dir/twiss_ac.dat', 0.0);
}else if(use_adt == 1){
exec, twiss_adt(0.275, 0.295, 0.263, 0.31, 1, '/home/eirik/CERN/correction_test/correction/model_dir/twiss_adt.dat', 0.0);
}
