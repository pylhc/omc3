title, "LHC Model created by omc3";
! Model directory: model_b1
! Natural Tune X         [     0.280]
! Natural Tune Y         [     0.310]
! Best Knowledge:        [        NO]
! Excitation             [       ACD]
! > Driven Tune X        [     0.280]
! > Driven Tune Y        [     0.310]


call, file = "rerun-create_data.py-for-working-script";
call, file = "rerun-create_data.py-for-working-script";
omc3_beam_energy = 6800.0;
exec, define_nominal_beams();

call, file = "rerun-create_data.py-for-working-script";

! ----- Calling Sequence -----
option, -echo;  ! suppress output from base sequence loading to keep the log small
call, file = "rerun-create_data.py-for-working-script";
option, echo;  ! re-enable output to see the optics settings

! ---- Call optics and other modifiers ----
call, file = "rerun-create_data.py-for-working-script"; !@modifier

! ----- Finalize Sequence -----
exec, cycle_sequences();
use, sequence = LHCB1;

! ----- Matching Knobs and Output Files -----
exec, match_tunes(0.28, 0.31, 1);
exec, coupling_knob(1);
exec, do_twiss_monitors(LHCB1, "rerun-create_data.py-for-working-script", 0.0);
exec, do_twiss_elements(LHCB1, "rerun-create_data.py-for-working-script", 0.0);
use_acd=1;
use_adt=0;
if(use_acd == 1){
exec, twiss_ac_dipole(0.28, 0.31, 0.28, 0.31, 1, "rerun-create_data.py-for-working-script", 0.0);
}else if(use_adt == 1){
exec, twiss_adt(0.28, 0.31, 0.28, 0.31, 1, "rerun-create_data.py-for-working-script", 0.0);
}
